{
  "name": "cipes",
  "version": "0.0.0",
  "description": "Git-Mining System mit Hilfe vonContinuous Integration",
  "main": "cipes.js",
  "scripts": {
    "lint": "eslint .",
    "build": "webpack",
    "dev": "webpack-dev-server -d --content-base src/ --inline --hot & npm run dev-server --",
    "dev-middle": "nodemon --exec babel-node --config .babelrc src/index.js",
    "dev-server": "nodemon --config .nodemonrc cipes.js --no-ci",
    "prod-middle": "babel src -d build && node build/index.js",
    "prod-server": "node cipes.js --no-ci"
  },
  "author": "Raphael Lackner <code@rala.io>",
  "license": "ISC",
