    "archiver": "^1.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.18.3",
